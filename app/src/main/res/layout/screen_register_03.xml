<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="418dp"
        android:layout_height="108dp"
        android:layout_marginTop="628dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/intersect" />

    <Button
        android:id="@+id/btnRegister_S02"
        android:layout_width="217dp"
        android:layout_height="48dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/buttton_sign_in"
        android:text="REGISTER"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassWordAgain_S02"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassWordAgain_S02"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassWordAgain_S02" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:drawableLeft="@drawable/grommet_icons_google"
        android:drawablePadding="10dp"
        android:text="REGISTER WITH GOOGLE"
        android:textColor="#5393f3"
        app:layout_constraintEnd_toEndOf="@+id/btnRegister_S02"
        app:layout_constraintStart_toStartOf="@+id/btnRegister_S02"
        app:layout_constraintTop_toBottomOf="@+id/btnRegister_S02" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="44dp"
        android:text="Already have an account?"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Sign in"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="218dp"
        android:layout_height="40dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="52dp"
        android:gravity="center"
        android:text="CREATE NEW ACCOUNT"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="67dp"
        android:layout_height="52dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView12"
        app:srcCompat="@drawable/group_3" />

    <EditText
        android:id="@+id/editTextTextName_S02"
        android:layout_width="319dp"
        android:layout_height="52dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="68dp"
        android:ems="10"
        android:hint="Your name"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <EditText
        android:id="@+id/editTextTextEmail_S02"
        android:layout_width="319dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextName_S02"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/editTextTextName_S02"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextName_S02" />

    <EditText
        android:id="@+id/editTextTextPassWord_S02"
        android:layout_width="319dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:hint="Type password"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextEmail_S02"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editTextTextEmail_S02"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmail_S02" />

    <EditText
        android:id="@+id/editTextTextPassWordAgain_S02"
        android:layout_width="319dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:hint="Type password"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassWord_S02"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassWord_S02"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassWord_S02" />

</androidx.constraintlayout.widget.ConstraintLayout>